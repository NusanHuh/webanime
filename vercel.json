{
    "version": 2,
    "builds": [
        { "src": "index.js", "use": "@vercel/node" }
    ],
    "routes": [
        { "src": "/api/(.*)", "dest": "index.js" },
        { "src": "/favicon.ico", "dest": "/public/favicon.ico" },
        { "src": "/(.*)", "dest": "/public/index.html" }
    ]
}  
